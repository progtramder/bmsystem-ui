.icon-file:before {
  content: "\e69c";
}

.icon-image:before {
  content: "\e61f";
}

.icon-video:before {
  content: "\e60a";
}

.icon-close:before {
  content: "\e7ac";
}


.editor-wrapper {
  width: 750rpx;
  background: #F2F6FC;
  padding: 20rpx;
  box-sizing: border-box;
}

.editor-add-wrapper {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 300rpx;
  height: 95rpx;
  margin: 0 auto;
}

.editor-add {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 75rpx;
  height: 75rpx;
  border: dashed 4rpx #C0C4CC;
  color: #C0C4CC;
  box-sizing: border-box;
}

.editor-node-wrapper {
  position: relative;
  width: 650rpx;
  margin: 0 auto;
}

.editor-textarea {
  width: 650rpx;
  border: solid 2rpx #E4E7ED;
  background: #fff;
  min-height: 100rpx;
  font-size: 32rpx;
  box-sizing: border-box;
}

.editor-delete {
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  top: -25rpx;
  right: -25rpx;
  border-radius: 50%;
  background: #F56C6C;
  color: #fff;
  z-index: 100;
}
.editor-image {
  display: block;
  width: 650rpx;
}

.editor-video {
  width: 100%;
  height: 500rpx;
}