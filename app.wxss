
/* page */
.container {
  display: flex;
  flex-direction: column;
  min-height: 100%;
  justify-content: space-between;
  font-size: 32rpx;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}

.page-body {
  width: 100%;
  flex-grow: 1;
  overflow-x: hidden;
}


@font-face {font-family: "iconfont";
  src: url('iconfont.eot?t=1545482741625'); /* IE9*/
  src: url('iconfont.eot?t=1545482741625#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
  url('iconfont.ttf?t=1545482741625') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('iconfont.svg?t=1545482741625#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-close:before { content: "\e62b"; }

.icon-text:before { content: "\e73e"; }

.icon-video:before { content: "\e656"; }

.icon-image:before { content: "\e73b"; }


.default-button {
  width: 90%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #0084FF;
  color: white;
}

.mask {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 101;
  background-color: black;
  opacity: 0.3;
}

.float-bar {
  background-color: #0084FF;
  color: white;
  position: fixed;
  width: 100%;
  height: 100rpx;
  top: 100%;
  left: 0;
  z-index: 100;
  transform: translateY(-100%);
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.float-button {
  position: fixed;
  width: 100rpx;
  height: 100rpx;
  color: white;
  line-height: 100rpx;
  bottom: 50rpx;
  right: 20rpx;
  font-size: 22rpx;
  border-radius: 50%;
  text-align: center;
}