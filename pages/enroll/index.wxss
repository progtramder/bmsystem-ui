.page-body {
  background-color: white;
}

.event-wrapper {
  display: flex;
  flex-direction: column;
  margin: 50rpx 0;
}
.nav-item {
  height: 120rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-style: solid;
  border-width: 5rpx;
  border-color:beige;
  margin: 0 50rpx 30rpx;
  border-radius: 10rpx;
  color: orange;
  font-weight: bold;
  font-size: 35rpx;
  background-color: #FBFBFB;
}
.icon {
  width: 50rpx;
  height: 50rpx;
  box-sizing: border-box;
  margin-right: 10rpx;
}
.picture {
  display: flex;
  justify-content: center;
  align-items: center;
}
.picture image {
  width: 750rpx;
}

.information {
  color: gray;
  text-align: center;
  font-style: italic;
  margin-top: 100rpx;
}